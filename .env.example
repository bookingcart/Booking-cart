# Amadeus API Configuration
# Get your credentials from: https://developers.amadeus.com/
AMADEUS_API_KEY=your_actual_api_key_here
AMADEUS_API_SECRET=your_actual_api_secret_here
AMADEUS_ENV=test

# Travel Buddy Visa Requirements API (RapidAPI)
# Get it from: https://rapidapi.com/TravelBuddyAI/api/visa-requirement
TRAVELBUDDY_RAPIDAPI_SECRET=your_rapidapi_proxy_secret_here

# Eventbrite API Configuration
# Get your token from: https://www.eventbrite.com/platform/api-keys/
EVENTBRITE_TOKEN=your_eventbrite_oauth_token_here

# Ticketmaster Discovery API Configuration
# Get your key from: https://developer.ticketmaster.com/
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here

# Duffel API Configuration (optional)
# Get your key from: https://duffel.com/
DUFFEL_API_KEY=your_duffel_api_key_here

PORT=3000
