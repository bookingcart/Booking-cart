{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "functions": {
    "api/duffel-search.js": {
      "runtime": "nodejs18.x"
    },
    "api/duffel-airports.js": {
      "runtime": "nodejs18.x"
    },
    "api/amadeus-airports.js": {
      "runtime": "nodejs18.x"
    },
    "api/events-search.js": {
      "runtime": "nodejs18.x"
    },
    "api/events-status.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
